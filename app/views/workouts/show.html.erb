<div data-controller="search-exercises">
  <%= form_with url: exercises_path, method: :get, html: { class:
    'mb-4', data: { search_exercises_target: 'form' } } do |f| %>
    <%= f.text_field :query,
                     class: "form-control",
                     placeholder: 'Search Exercise',
                     value: params[:query],
                     data: { search_exercises_target: 'input'
                             action: 'keyup->search-exercises#update'} %>


